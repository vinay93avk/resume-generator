<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Generated Resume</title>
</head>
<body>
    <h1>Resume for <%= firstName %> <%= lastName %></h1>
    <p>Email: <%= email %></p>
    <p>Phone: <%= phone %></p>
    <h2>Education</h2>
    <p><%= education %></p>
    <h2>Experience</h2>
    <ul>
        <% experience.forEach(function(point) { %>
            <li><%= point %></li>
        <% }); %>
    </ul>
    <h2>Skills</h2>
    <p><%= skills %></p>
    <h2>LinkedIn URL</h2>
    <p><%= linkedUrl %></p>
</body>
</html>
